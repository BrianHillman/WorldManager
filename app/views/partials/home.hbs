<ul class="thumbnails">
{{#each preview}} 
	<li class="span3">
		<a href={{this.href}} class="thumbnail"><img src={{this.img}} alt={{this.name}} alt2={{this.info}}/></a>
		<div class = "caption pagination-centered">
			<h3>{{this.name}}</h3>
			<p>{{this.name}}</p>
			<p>{{this.info}}</p>
		</div>
	</li>
{{/each}}
</ul>