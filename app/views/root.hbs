					<div class = "row">
						<h1>Virtual Worlds <small>World Manager</small> </h1>
					</div>
					<div class = "row-fluid">
						<a class = "btn btn-danger"	href="/">Home</a>
						<a class="btn btn-danger" href="/">About</a>
						<a class="btn btn-danger" href="/">Contact</a>
